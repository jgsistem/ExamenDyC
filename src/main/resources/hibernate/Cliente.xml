<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dycs.customers.domain.entity">
	<class name="Cliente" table="cliente">
        <id name="id" column="cliente_id">
            <generator class="native"/>
        </id>
        <property name="firstName" column="first_name" not-null="true"/>
        <property name="lastName" column="last_name" not-null="true"/>
        <property name="rol" column="rol" not-null="true"/>        
        <property name="isActive" column="active" not-null="true"/>        
        
    </class> 
</hibernate-mapping>